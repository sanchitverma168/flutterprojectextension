{
	"name": "flutter-project-directory",
	"displayName": "Flutter Project Directory",
	"description": "This is a extension which will create you the sample project directory for your files in flutter project.",
	"version": "0.0.3",
	"icon": "images/logo.png",
	"engines": {
		"vscode": "^1.57.0"
	},
	"publisher": "SanchitVerma",
	"categories": [
		"Other"
	],
	"keywords": [
		"flutter",
		"snippets",
		"templates",
		"extension",
		"flutter project",
		"intellisense",
		"snip",
		"bloc",
		"provider",
		"dart",
		"state",
		"management"

	],
	"activationEvents": [
		"onCommand:flutter-project-directory.createfolders",
		"onCommand:flutter-project-directory.createblocfolders"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [{
			"command": "flutter-project-directory.createfolders",
			"title": "FPD:: Create Folders for Flutter Project"
		}, {
			"command": "flutter-project-directory.createblocfolders",
			"title": "FPD:: Create Bloc Folders for Flutter Project"
		}]
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/sanchitverma168/flutterprojectextension"
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "npm run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.57.0",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.2.2",
		"@types/node": "14.x",
		"eslint": "^7.27.0",
		"glob": "^7.1.7",
		"mocha": "^8.4.0",
		"typescript": "^4.3.2",
		"vscode-test": "^1.5.2"
	},
	"dependencies": {
		"fs": "0.0.1-security"
	}
}